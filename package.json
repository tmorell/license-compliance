{
    "author": "Teo Morell <teomorell@gmail.com>",
    "ava": {
        "compileEnhancements": false,
        "extensions": [
            "ts"
        ],
        "files": [
            "!test/util.ts"
        ],
        "require": [
            "ts-node/register"
        ],
        "tap": false,
        "timeout": "10000",
        "verbose": true
    },
    "bin": {
        "license-compliance": "./bin/cli.js"
    },
    "bugs": {
        "url": "https://github.com/tmorell/license-compliance/issues"
    },
    "dependencies": {
        "chalk": "2.4.2",
        "commander": "4.0.0",
        "debug": "4.1.1",
        "spdx-expression-parse": "3.0.0",
        "spdx-satisfies": "5.0.0"
    },
    "description": "License compliance checker",
    "devDependencies": {
        "@types/debug": "4.1.5",
        "@types/node": "12.12.6",
        "@types/sinon": "7.5.0",
        "ava": "2.4.0",
        "nyc": "14.1.1",
        "sinon": "8.1.1",
        "ts-node": "8.4.1",
        "tsc-watch": "4.0.0",
        "tslint": "5.20.1",
        "tslint-checks": "1.0.0",
        "typescript": "3.6.4"
    },
    "engines": {
        "node": ">=8.16.2"
    },
    "files": [
        "/lib/**/*",
        "/bin/**/*"
    ],
    "homepage": "https://github.com/tmorell/license-compliance#readme",
    "keywords": [
        "checker",
        "cli",
        "compliance",
        "license"
    ],
    "license": "MIT",
    "main": "./lib/index.js",
    "name": "license-compliance",
    "nyc": {
        "all": true,
        "cache": false,
        "exclude": [
            "**/*.d.ts",
            "**/*.spec.ts",
            "coverage",
            "baseline",
            "test/util.ts"
        ],
        "extension": [
            ".ts"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tmorell/license-compliance.git"
    },
    "scripts": {
        "build": "rm -rf ./lib ./.nyc_output ./coverage && tsc",
        "coverage": "nyc report --reporter=html && xdg-open ./coverage/index.html",
        "coverage:ci": "nyc report --reporter=lcov",
        "debug:watch": "DEBUG=license-checker:* DEBUG_COLORS=true tsc-watch --onSuccess 'node ./lib/index.js'",
        "nyc": "nyc ava",
        "test": "npm run build && npm run nyc",
        "test:watch": "ava --watch",
        "tslint": "tslint -t stylish -c tslint.json -p tsconfig.spec.json --force"
    },
    "version": "1.0.1"
}
